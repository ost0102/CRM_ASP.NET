#CrmCustomerMgt@CheckCustCode
--CrmCustomerMgt@CheckCustCodeSELECT *   FROM CRM_CUST_MST WHERE CUST_CD = '@CUST_CD'
#CrmCustomerMgt@DBInfo
--CrmCustomerMgt@DBInfo[ELVIS]  SELECT 'N' AS CHK       , USR_ID       , USR_PWD       , DB_IP       , DB_IP_EXT       , DB_PORT       , DB_SID       , CUST_NM       , DOMAIN       , CTRY_CD       , '' AS RESULT   FROM SYS_CUST_MST@ELVIS  WHERE USE_YN    = 'Y'    AND DOMAIN    = '@DOMAIN' [TB_CHG_YN]   AND TB_CHG_YN = 'Y' [/TB_CHG_YN][/ELVIS][ELVIS_PLUS]  SELECT 'N' AS CHK       , USR_ID       , USR_PWD       , DB_IP       , DB_IP_EXT       , DB_PORT       , DB_SID       , CUST_NM       , DOMAIN       , CTRY_CD       , '' AS RESULT   FROM SYS_CUST_MST@ELVIS_PLUS WHERE USE_YN    = 'Y'    AND DOMAIN    = '@DOMAIN' [TB_CHG_YN]   AND TB_CHG_YN = 'Y' [/TB_CHG_YN][/ELVIS_PLUS]  
#CrmCustomerMgt@Delete
-- CrmCustomerMgt@DeleteBEGIN   DELETE CRM_CUST_MST WHERE CUST_CD ='@CUST_CD';   DELETE CRM_CUST_EDI WHERE CUST_CD ='@CUST_CD';   DELETE CRM_CUST_PIC WHERE CUST_CD ='@CUST_CD';   DELETE CRM_CUST_DTL WHERE CUST_CD ='@CUST_CD';   DELETE CRM_CUST_SVC WHERE CUST_CD ='@CUST_CD';   DELETE CRM_CUST_RMK WHERE CUST_CD ='@CUST_CD';   DELETE CRM_CUST_SERVER WHERE CUST_CD ='@CUST_CD';     DELETE CRM_CUST_FAX WHERE CUST_CD ='@CUST_CD';     DELETE CRM_CUST_JOIN WHERE CUST_CD ='@CUST_CD';     END;
#CrmCustomerMgt@DeleteBilling
--CrmCustomerMgt@DeleteBillingDELETE   FROM CRM_CUST_FAX WHERE CUST_CD = '@CUST_CD'    AND SEQ     = @SEQ
#CrmCustomerMgt@DeleteCustInfo
DELETE CRM_CUST_DTL WHERE CUST_CD = '@CUST_CD'   AND ITEM_CD = '@ITEM_CD'	
#CrmCustomerMgt@DeleteCustPic
DELETE CRM_CUST_PIC WHERE CUST_CD = '@CUST_CD'   AND SEQ = @SEQ	
#CrmCustomerMgt@DeleteEDIInfo
-- CrmCustomerMgt@DeleteEDIInfoDELETE   FROM CRM_CUST_EDI WHERE CUST_CD = '@CUST_CD'   AND EDI_SYS_CD = '@EDI_SYS_CD'
#CrmCustomerMgt@DeleteJoin
--CrmCustomerMgt@DeleteJoinDELETE   FROM CRM_CUST_JOIN WHERE CUST_CD = '@CUST_CD'    AND SEQ     = @SEQ
#CrmCustomerMgt@DeleteRemark
--CrmCustomerMgt@DeleteRemarkDELETE   FROM CRM_CUST_RMK WHERE CUST_CD = '@CUST_CD'    AND SEQ     = @SEQ
#CrmCustomerMgt@DeleteSalesRemark
--CrmCustomerMgt@DeleteSalesRemarkDELETE   FROM CRM_CUST_RMK_SALES WHERE CUST_CD = '@CUST_CD'    AND SEQ     = @SEQ
#CrmCustomerMgt@DeleteServer
-- CrmCustomerMgt@DeleteServerDELETE CRM_CUST_SERVER  WHERE CUST_CD='@CUST_CD'   AND SEQ=@SEQ
#CrmCustomerMgt@DeleteService
--CrmCustomerMgt@DeleteServiceDELETE   FROM CRM_CUST_SVC WHERE CUST_CD = '@CUST_CD'    AND SEQ     = @SEQ
