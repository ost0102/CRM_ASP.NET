#CrmCustomerMgt@InsertRPAAuthKey
--CrmCustomerMgt@InsertRPAAuthKeyBEGIN    UPDATE RPA_AUTH_KEY --@DL_RPA	   SET USE_YN = 'N'	 WHERE CUST_CD = '@CUST_CD'	   AND USE_YN = 'Y';	   	INSERT INTO RPA_AUTH_KEY ( --@DL_RPA	       MNGT_NO	     , INS_USR	     , INS_YMD	     , INS_HM	     [CUST_CD], CUST_CD[/CUST_CD]	     [CRN], CRN[/CRN]	     [CUSTOMS_CD], CUSTOMS_CD[/CUSTOMS_CD]	     [FM_YMD], FM_YMD[/FM_YMD]	     [TO_YMD], TO_YMD[/TO_YMD]	     [AUTH_KEY], AUTH_KEY[/AUTH_KEY]	     [USE_YN], USE_YN[/USE_YN]	) VALUES (	       '@MNGT_NO'	     , '@USR_ID'         , TO_CHAR(SYSDATE,'yyyyMMdd')         , TO_CHAR(SYSDATE,'HH24MISS')	     [CUST_CD], '@CUST_CD'[/CUST_CD]	     [CRN], '@CRN'[/CRN]	     [CUSTOMS_CD], '@CUSTOMS_CD'[/CUSTOMS_CD]	     [FM_YMD], '@FM_YMD'[/FM_YMD]	     [TO_YMD], '@TO_YMD'[/TO_YMD]	     [AUTH_KEY], '@AUTH_KEY'[/AUTH_KEY]	     [USE_YN], '@USE_YN'[/USE_YN]	);END;
