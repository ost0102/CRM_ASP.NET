--CrmCustomerMgt@UpdateRemarkUPDATE CRM_CUST_RMK   SET UPD_USR  = '@USR_ID'	 , UPD_DATE = SYSDATE		 [RMK], RMK = '@RMK'[/RMK]	 [RMK_YMD], RMK_YMD = '@RMK_YMD'[/RMK_YMD]	 [RMK_TYPE], RMK_TYPE = '@RMK_TYPE'[/RMK_TYPE]WHERE CUST_CD = '@CUST_CD'  AND SEQ     = @SEQ    
#CrmCustomerMgt@UpdateRemark_20170103
-- CrmCustomerMgt@UpdateRemarkDECLARE    RMK_STR VARCHAR2(32767);BEGIN	RMK_STR :='@RMK';		UPDATE CRM_CUST_MST	   SET RMK=RMK_STR	 WHERE CUST_CD = '@CUST_CD';END;
#CrmCustomerMgt@UpdateSalesRemark
--CrmCustomerMgt@UpdateSalesRemarkUPDATE CRM_CUST_RMK_SALES   SET UPD_USR  = '@USR_ID'	 , UPD_DATE = SYSDATE		 [RMK], RMK = '@RMK'[/RMK]	 [RMK_YMD], RMK_YMD = '@RMK_YMD'[/RMK_YMD]	 [RMK_TYPE], RMK_TYPE = '@RMK_TYPE'[/RMK_TYPE]WHERE CUST_CD = '@CUST_CD'  AND SEQ     = @SEQ
#CrmCustomerMgt@UpdateServer
-- CrmCustomerMgt@UpdateServerUPDATE CRM_CUST_SERVER    SET SVR_TYPE='@SVR_TYPE'	, SVR_IP=PKG_CRYPTO.ENCRYPT('@SVR_IP','@CRYPTO_KEY' || 'Key!//#^') 	, SVR_DESC='@SVR_DESC'	, WIN_ID=PKG_CRYPTO.ENCRYPT('@WIN_ID','@CRYPTO_KEY' || 'Key!//#^') 	, WIN_PWD=PKG_CRYPTO.ENCRYPT('@WIN_PWD','@CRYPTO_KEY' || 'Key!//#^') 	, DB_ID=PKG_CRYPTO.ENCRYPT('@DB_ID','@CRYPTO_KEY' || 'Key!//#^') 	, DB_PWD=PKG_CRYPTO.ENCRYPT('@DB_PWD','@CRYPTO_KEY' || 'Key!//#^') 	, DB_PORT =PKG_CRYPTO.ENCRYPT('@DB_PORT','@CRYPTO_KEY' || 'Key!//#^') 	, DB_SID =PKG_CRYPTO.ENCRYPT('@DB_SID','@CRYPTO_KEY' || 'Key!//#^') 	, DB_TNS =PKG_CRYPTO.ENCRYPT('@DB_TNS','@CRYPTO_KEY' || 'Key!//#^') 	, TV_ID=PKG_CRYPTO.ENCRYPT('@TV_ID','@CRYPTO_KEY' || 'Key!//#^') 	, TV_PWD=PKG_CRYPTO.ENCRYPT('@TV_PWD','@CRYPTO_KEY' || 'Key!//#^') 	, RMK='@RMK'	, ORA_VER='@ORA_VER'	, MAIN_YN='@MAIN_YN'		, UPD_USR='@USR_ID'	, UPD_DATE=SYSDATE	WHERE CUST_CD='@CUST_CD'  AND SEQ=@SEQ
#CrmCustomerMgt@UpdateService
--CrmCustomerMgt@UpdateServiceUPDATE CRM_CUST_SVC   SET UPD_USR  = '@USR_ID'	 , UPD_DATE = SYSDATE		 [CTRT_STRT_YMD], CTRT_STRT_YMD = '@CTRT_STRT_YMD'[/CTRT_STRT_YMD]	 [CTRT_END_YMD], CTRT_END_YMD = '@CTRT_END_YMD'[/CTRT_END_YMD]	 [SVC_CD], SVC_CD = '@SVC_CD'[/SVC_CD]	 [DEPT_CD], DEPT_CD = '@DEPT_CD'[/DEPT_CD]	 [SALES_NM], SALES_NM = '@SALES_NM'[/SALES_NM]	 [RMK], RMK = '@RMK'[/RMK]	 [TAX_AMT], TAX_AMT = '@TAX_AMT'[/TAX_AMT]	 [TAX_START_YMD], TAX_START_YMD = '@TAX_START_YMD'[/TAX_START_YMD]WHERE CUST_CD = '@CUST_CD'  AND SEQ     = @SEQ    
