--CrmCustomerMgt@InsertSalesRemarkINSERT INTO CRM_CUST_RMK_SALES(	  CUST_CD	, SEQ	, INS_USR	, INS_DATE	, UPD_USR	, UPD_DATE	[RMK], RMK[/RMK]	[RMK_YMD], RMK_YMD[/RMK_YMD]	[RMK_TYPE], RMK_TYPE[/RMK_TYPE]) VALUES (	  '@CUST_CD' 	, (NVL((SELECT MAX(B.SEQ) FROM CRM_CUST_RMK_SALES B WHERE B.CUST_CD = '@CUST_CD'),0)+1)	, '@USR_ID'	, SYSDATE	, '@USR_ID'	, SYSDATE	[RMK], '@RMK'[/RMK]		[RMK_YMD], '@RMK_YMD'[/RMK_YMD]	[RMK_TYPE], '@RMK_TYPE'[/RMK_TYPE])
#CrmCustomerMgt@InsertServer
-- CrmCustomerMgt@InsertServerINSERT INTO CRM_CUST_SERVER (	  CUST_CD	, SEQ	, SVR_TYPE	, SVR_IP	, SVR_DESC	, WIN_ID	, WIN_PWD	, DB_ID	, DB_PWD	, DB_PORT	, DB_SID	, DB_TNS 	, TV_ID	, TV_PWD	, RMK	, ORA_VER	, MAIN_YN	, INS_USR	, INS_DATE) SELECT	 '@CUST_CD'	,(SELECT NVL(MAX(SEQ),0)+1 AS SEQ	   	    FROM CRM_CUST_SERVER		   WHERE CUST_CD = '@CUST_CD') 	, '@SVR_TYPE'	, PKG_CRYPTO.ENCRYPT('@SVR_IP','@CRYPTO_KEY' || 'Key!//#^') 	, '@SVR_DESC'	, PKG_CRYPTO.ENCRYPT('@WIN_ID','@CRYPTO_KEY' || 'Key!//#^') 	, PKG_CRYPTO.ENCRYPT('@WIN_PWD','@CRYPTO_KEY' || 'Key!//#^') 	, PKG_CRYPTO.ENCRYPT('@DB_ID','@CRYPTO_KEY' || 'Key!//#^') 	, PKG_CRYPTO.ENCRYPT('@DB_PWD','@CRYPTO_KEY' || 'Key!//#^') 	, PKG_CRYPTO.ENCRYPT('@DB_PORT','@CRYPTO_KEY' || 'Key!//#^') 	, PKG_CRYPTO.ENCRYPT('@DB_SID','@CRYPTO_KEY' || 'Key!//#^') 	, PKG_CRYPTO.ENCRYPT('@DB_TNS','@CRYPTO_KEY' || 'Key!//#^') 	, PKG_CRYPTO.ENCRYPT('@TV_ID','@CRYPTO_KEY' || 'Key!//#^') 	, PKG_CRYPTO.ENCRYPT('@TV_PWD','@CRYPTO_KEY' || 'Key!//#^') 	, '@RMK'	, '@ORA_VER'	, '@MAIN_YN'	, '@USR_ID'	, SYSDATE FROM DUAL
#CrmCustomerMgt@InsertService
--CrmCustomerMgt@InsertServiceINSERT INTO CRM_CUST_SVC(	  CUST_CD	, SEQ	, INS_USR	, INS_DATE	, UPD_USR	, UPD_DATE	[CTRT_STRT_YMD], CTRT_STRT_YMD [/CTRT_STRT_YMD]	[CTRT_END_YMD], CTRT_END_YMD [/CTRT_END_YMD]	[SVC_CD], SVC_CD[/SVC_CD]	[DEPT_CD], DEPT_CD[/DEPT_CD]	[SALES_NM], SALES_NM[/SALES_NM]	[RMK], RMK[/RMK]	[TAX_AMT], TAX_AMT [/TAX_AMT]	[TAX_START_YMD], TAX_START_YMD [/TAX_START_YMD]) VALUES (	  '@CUST_CD' 	, (NVL((SELECT MAX(B.SEQ) FROM CRM_CUST_SVC B WHERE B.CUST_CD = '@CUST_CD'),0)+1)	, '@USR_ID'	, SYSDATE	, '@USR_ID'	, SYSDATE	[CTRT_STRT_YMD], '@CTRT_STRT_YMD' [/CTRT_STRT_YMD]	[CTRT_END_YMD], '@CTRT_END_YMD' [/CTRT_END_YMD]	[SVC_CD], '@SVC_CD'[/SVC_CD]	[DEPT_CD], '@DEPT_CD'[/DEPT_CD]	[SALES_NM], '@SALES_NM'[/SALES_NM]	[RMK], '@RMK'[/RMK]		[TAX_AMT], '@TAX_AMT' [/TAX_AMT]	[TAX_START_YMD], '@TAX_START_YMD' [/TAX_START_YMD])
#CrmCustomerMgt@KeyCreate
-- CrmCustomerMgt@KeyCreate  SELECT LPAD(CRM_CUST_MST_SEQ.NEXTVAL,6,'0') AS CUST_CD    FROM DUAL 
#CrmCustomerMgt@Print
--CrmCustomerMgt@PrintSELECT  A.CUST_CD      , CUST_NM      , A.TEL_NO      , UFN_FORMAT(A.CTRT_STRT_YMD,'DATE') AS CTRT_STRT_YMD        , SVC_CD      , INV_DAY      , RCV_DAY      , MATN_AMT      , JOIN_ROUTE      , JOIN_RMK      , '@USER_NM' AS USER_NM      , ETC_AMT_INFO      , CTRT_RMK      , B.PIC_NM AS PIC_NM_Y      , B.EMAIL AS EMAIL_Y      , B.TEL_NO AS TEL_NO_Y      , C.PIC_NM AS PIC_NM_A      , C.EMAIL AS EMAIL_A      , C.TEL_NO AS TEL_NO_A  FROM CRM_CUST_MST A       LEFT OUTER JOIN CRM_CUST_PIC B       ON A.CUST_CD = B.CUST_CD       AND B.MAIN_YN = 'Y'        LEFT OUTER JOIN CRM_CUST_PIC C       ON A.CUST_CD = C.CUST_CD       AND C.MAIN_YN = 'A' WHERE A.CUST_CD = '@CUST_CD'
#CrmCustomerMgt@SearchBilling
--CrmCustomerMgt@SearchBillingSELECT A.CUST_CD     , A.SEQ     , A.SVC_TYPE     , A.VENDER_CD     , A.FAX_NO     , A.START_YMD     , A.END_YMD     , A.RMK     , A.BASE_RATE_AMT     , A.RATE_AMT     , A.FREE_CNT     , (SELECT B.LOC_NM FROM CRM_USR_MST B WHERE B.USR_ID = A.INS_USR) AS INS_USR     , TO_CHAR(A.INS_DATE,'YYYY-MM-DD HH24:MI:SS') AS INS_DATE     , (SELECT B.LOC_NM FROM CRM_USR_MST B WHERE B.USR_ID = A.INS_USR) AS UPD_USR     , TO_CHAR(A.UPD_DATE,'YYYY-MM-DD HH24:MI:SS') AS UPD_DATE  FROM CRM_CUST_FAX A WHERE A.CUST_CD = '@CUST_CD' [SVC_TYPE]   AND A.SVC_TYPE = '@SVC_TYPE' [/SVC_TYPE]
#CrmCustomerMgt@SearchBillingAmt
--CrmCustomerMgt@SearchBillingAmtSELECT A.CRM_CD AS CUST_CD     , A.SVC_TYPE     , A.BILL_MONTH     , A.PAGE_CNT     , A.BASE_RATE_AMT     , A.RATE_AMT     , A.BILLING_AMT     , A.FREE_CNT     , A.FAX_NO  FROM VT_CRM_CUST_FAX A WHERE A.CRM_CD = '@CUST_CD' 
#CrmCustomerMgt@SearchCustInfo
SELECT  CUST_CD       , ITEM_CD	  , ITEM_VAL   FROM CRM_CUST_DTL WHERE CUST_CD = '@CUST_CD' 	 	  
#CrmCustomerMgt@SearchCustPic
SELECT *  FROM CRM_CUST_PIC WHERE CUST_CD = '@CUST_CD' ORDER BY PIC_NM
#CrmCustomerMgt@SearchDetail
--CrmCustomerMgt@SearchDetailSELECT  A.CUST_CD      , A.CUST_NM      , A.CUST_ADDR      , A.CUST_LOC_NM      , A.TEL_NO      , A.FAX_NO      --, SVC_CD      , A.DOMAIN      , A.USE_YN      --, SALES_NM      , A.CTRT_STRT_YMD      , A.CTRT_END_YMD      , A.CTRT_END_RSN      , A.CTRT_AMT      , A.MATN_AMT      , A.IDC_AMT      , A.LICENSE_AMT      , A.USER_QTY      , A.STLE_YMD      , A.BILG_YMD      , A.TBL_CHG_YN      , REPLACE(A.CRN, '-', '') AS CRN      , A.CEO      , A.NEW_CUST_YN      , A.CTRT_CD      , A.MATN_YN      , A.SETUP_URL      , A.SVN      , A.ADMIN_ID      , A.ADMIN_PWD      , A.CUSTOMS_CD      , A.CTRY_CD      , A.LANG_CD      , A.RMK      , A.INS_USR      , A.INS_YMD      , A.INS_HM      , A.UPD_USR      , A.UPD_YMD      , A.UPD_HM      , A.E_MAIL      , A.CUST_TYPE      , A.BILG_CD      , A.ETC_AMT      , A.ETC_AMT_INFO      , A.CTRT_RMK      , A.ZIP_CD      , A.JOIN_ROUTE      , A.INV_DAY      , A.RCV_DAY      , A.BIZ_TYPE      , A.BIZ_COND      , A.MIG_CUST_YN      , A.MIG_USER_YN      , A.MIG_ACT_YN      , A.FOUND_YMD      , A.JOIN_RMK      --, DEPT_CD      --,(SELECT CD_NM FROM MDM_COM_CODE WHERE GRP_CD = 'C03' AND COMN_CD = DEPT_CD) AS DEPT_NM      ,(SELECT SUM(Z.OUTSTAND_AMT) FROM ACT_CUST_OUTSTAND Z WHERE Z.CRN = REPLACE(A.CRN, '-', '')) AS OUTSTAND_AMT      ,(SELECT MIN(Z.TAX_START_YMD) FROM ACT_CUST_OUTSTAND Z WHERE Z.CRN = REPLACE(A.CRN, '-', '')) AS TAX_START_YMD      ,(SELECT MAX(Z.TAX_AMT) FROM ACT_CUST_OUTSTAND Z WHERE Z.CRN = REPLACE(A.CRN, '-', '')) AS TAX_AMT      ,(SELECT MAX(Z.RECV_END_YMD) FROM ACT_CUST_OUTSTAND Z WHERE Z.CRN = REPLACE(A.CRN, '-', '')) AS RECV_END_YMD      , A.STOP_YMD      , A.CREDIT_DAYS      , A.CREDIT_AMT      , A.CREDIT_YMD      , A.CREDIT_YMD AS DB_CREDIT_YMD      , A.CREDIT_PIC_NM	  , A.BANK_YN	  , A.BANK_TYPE	  , A.BANK_RCV_YN	  , A.BANK_RMK	  , A.BANK_DAY      , A.CHAIN_ID      , A.CHAIN_PWD      , A.KEY_WORD      , A.DEV_PIC      , A.CHAIN_START_YMD      , A.CHAIN_END_YMD      , A.RMK_AR      , A.AR_TYPE      , A.ISSUE_TYPE      , A.CS_SETUP_KEY      , A.CS_SETUP_CNT      , A.DEPT_CD      , A.ELVIS_OFFICE_CD      , A.TAX_TRANS_TYPE      , A.TAX_ID      , A.TAX_PWD  FROM CRM_CUST_MST A  WHERE A.CUST_CD='@CUST_CD'     
#CrmCustomerMgt@SearchEDIInfo
-- CrmCustomerMgt@SearchEDIInfoSELECT A.CUST_CD     , A.EDI_SYS_CD     , A.EDI_SYS_NM     , A.ID     , A.PWD     , A.SENDER_ID     , A.MAIL_BOX_ID     , B.RMK     , A.INS_USR     , A.INS_YMD     , A.INS_HM     , A.UPD_USR     , A.UPD_YMD     , A.UPD_HM  FROM CRM_CUST_EDI A         LEFT OUTER JOIN MDM_COM_CODE B                       ON B.COMN_CD = A.EDI_SYS_CD                     AND B.GRP_CD = 'M42' WHERE 1=1   AND A.CUST_CD = '@CUST_CD' [EDIINFO_Y]   AND B.OPT_ITEM6 = 'Y' [/EDIINFO_Y] ORDER BY B.CD_NM
#CrmCustomerMgt@SearchEDIInfoCount
-- CrmCustomerMgt@SearchEDIInfoCountSELECT A.CUST_CD     , A.EDI_SYS_CD  FROM CRM_CUST_EDI A WHERE A.CUST_CD = '@CUST_CD'   AND A.EDI_SYS_CD = '@EDI_SYS_CD'
#CrmCustomerMgt@SearchJoin
--CrmCustomerMgt@SearchJionSELECT A.CUST_CD     , A.SEQ     , A.SVC_TYPE     , A.VENDER_CD     , A.SEND_YMD     , A.START_YMD     , A.END_YMD     , A.RMK     , (SELECT B.LOC_NM FROM CRM_USR_MST B WHERE B.USR_ID = A.INS_USR) AS INS_USR     , TO_CHAR(A.INS_DATE,'YYYY-MM-DD HH24:MI:SS') AS INS_DATE     , (SELECT B.LOC_NM FROM CRM_USR_MST B WHERE B.USR_ID = A.INS_USR) AS UPD_USR     , TO_CHAR(A.UPD_DATE,'YYYY-MM-DD HH24:MI:SS') AS UPD_DATE  FROM CRM_CUST_JOIN A WHERE A.CUST_CD = '@CUST_CD' [SVC_TYPE]   AND A.SVC_TYPE = '@SVC_TYPE' [/SVC_TYPE]
#CrmCustomerMgt@SearchMain
--CrmCustomerMgt@SearchMainSELECT  A.CUST_CD      , NVL((SELECT MAX(B.OUTSTAND_AMT) FROM VT_CRM_CUST_MST_BK B WHERE A.CRN = B.CRN AND A.CUST_CD = B.CUST_CD),0) AS OUTSTAND_AMT      , NVL((SELECT MAX(B.TAX_AMT) FROM VT_CRM_CUST_MST_BK B WHERE A.CRN = B.CRN AND A.CUST_CD = B.CUST_CD),0) AS TAX_AMT --사업자번호 000-00-00000 입력한 자료는 중복조회될수 있음      , (SELECT MAX(B.TAX_START_YMD) FROM VT_CRM_CUST_MST_BK B WHERE A.CRN = B.CRN AND A.CUST_CD = B.CUST_CD) AS TAX_START_YMD      , (SELECT MAX(B.CHECK_YN) FROM VT_CRM_CUST_MST_BK B WHERE A.CRN = B.CRN AND A.CUST_CD = B.CUST_CD) AS CHECK_YN      , A.CUST_LOC_NM AS CUST_NM      , A.USE_YN      , (SELECT DISTINCT WM_CONCAT(UFN_TABLE_ITEM('COM_CODE',C.SVC_CD,'C01','')) FROM CRM_CUST_SVC C WHERE C.CUST_CD = A.CUST_CD AND C.CTRT_END_YMD IS NULL) AS SVC_CD      , (SELECT MAX(C.SALES_NM) FROM CRM_CUST_SVC C WHERE C.CUST_CD = A.CUST_CD AND C.CTRT_END_YMD IS NULL) AS SALES_NM      , A.TEL_NO      , A.FAX_NO      , A.E_MAIL      , PKG_CRYPTO.DECRYPT((SELECT MAX(X.SVR_IP) FROM CRM_CUST_SERVER X WHERE CUST_CD = A.CUST_CD AND SVR_TYPE = 'D' AND MAIN_YN = 'Y'),'@CRYPTO_KEY' || 'Key!//#^') AS DB_IP      , PKG_CRYPTO.DECRYPT(        (SELECT MAX(X.SVR_IP)           FROM CRM_CUST_SERVER X          WHERE CUST_CD = A.CUST_CD            AND SVR_TYPE = 'D'            AND X.SVR_IP IN (PKG_CRYPTO.ENCRYPT('IDCDB01.LOGIS21.COM','@CRYPTO_KEY' || 'Key!//#^'),PKG_CRYPTO.ENCRYPT('IDCDB02.LOGIS21.COM','@CRYPTO_KEY' || 'Key!//#^'),PKG_CRYPTO.ENCRYPT('IDCDB03.LOGIS21.COM','@CRYPTO_KEY' || 'Key!//#^'),PKG_CRYPTO.ENCRYPT('IDCDB04.LOGIS21.COM','@CRYPTO_KEY' || 'Key!//#^'))          ),'@CRYPTO_KEY' || 'Key!/#^') AS DB_IP_CS      , PKG_CRYPTO.DECRYPT((SELECT MAX(X.SVR_IP)           FROM CRM_CUST_SERVER X          WHERE CUST_CD = A.CUST_CD            AND SVR_TYPE = 'A'            AND MAIN_YN = 'Y'         ),'@CRYPTO_KEY' || 'Key!//#^') AS APP_IP      , A.DOMAIN      , A.MATN_YN      , (SELECT X.PIC_NM           FROM CRM_CUST_PIC X         WHERE X.CUST_CD = A.CUST_CD           AND X.MAIN_YN = 'Y'           AND ROWNUM = 1         ) AS PIC_NM      , (SELECT X.EMAIL           FROM CRM_CUST_PIC X         WHERE X.CUST_CD = A.CUST_CD           AND X.MAIN_YN = 'Y'           AND ROWNUM = 1         ) AS PIC_EMAIL      , A.CUSTOMS_CD         , A.CTRY_CD      , A.LANG_CD          , (SELECT MAX(C.CTRT_STRT_YMD)                 FROM CRM_CUST_SVC C                           WHERE C.CUST_CD = A.CUST_CD                  AND C.CTRT_END_YMD IS NULL) AS CTRT_STRT_YMD      , (SELECT MAX(C.CTRT_END_YMD)                 FROM CRM_CUST_SVC C                           WHERE C.CUST_CD = A.CUST_CD                  AND C.CTRT_END_YMD IS NULL) AS CTRT_END_YMD      , A.CTRY_CD      , CASE WHEN (SELECT COUNT(X.MNGT_NO)                    FROM COM_DOC_MST X                    WHERE X.MNGT_NO = A.CUST_CD) >= 1             THEN 'Y'             ELSE 'N'             END AS FILE_YN      , A.CUST_TYPE      , (SELECT X.PIC_NM           FROM CRM_CUST_PIC X         WHERE X.CUST_CD = A.CUST_CD           AND X.MAIN_YN = 'A'           AND ROWNUM = 1         ) AS ACT_PIC_NM      , (SELECT X.TEL_NO           FROM CRM_CUST_PIC X         WHERE X.CUST_CD = A.CUST_CD           AND X.MAIN_YN = 'A'           AND ROWNUM = 1         ) AS ACT_PIC_TEL      , (SELECT X.EMAIL           FROM CRM_CUST_PIC X         WHERE X.CUST_CD = A.CUST_CD           AND X.MAIN_YN = 'A'           AND ROWNUM = 1         ) AS ACT_PIC_EMAIL      , (SELECT MAX(PKG_CRYPTO.DECRYPT(X.SVR_IP,'@CRYPTO_KEY' || 'Key!//#^') || ':' || PKG_CRYPTO.DECRYPT(X.DB_PORT,'@CRYPTO_KEY' || 'Key!//#^'))           FROM CRM_CUST_SERVER X          WHERE CUST_CD = A.CUST_CD            AND SVR_TYPE = 'A'            AND MAIN_YN = 'Y'         ) AS APP_IP_PORT      , A.FOUND_YMD       , A.BIZ_TYPE       , A.BIZ_COND       , A.INV_DAY       , A.RCV_DAY       , A.JOIN_ROUTE       , A.JOIN_RMK      , A.MIG_CUST_YN      , A.MIG_USER_YN      , A.MIG_ACT_YN          --, (SELECT CD_NM FROM MDM_COM_CODE WHERE GRP_CD = 'C03' AND COMN_CD = A.DEPT_CD) AS DEPT_CD     --, SVC.DEPT_CD      , '' AS INFO_YN      , '' AS STOP_YN        , A.INFO_YMD      , A.STOP_YMD        , A.CREDIT_YMD      , A.CREDIT_PIC_NM      , A.BANK_YN      , A.USER_QTY      , A.CRN      , A.CHAIN_ID      , A.KEY_WORD      , A.DEV_PIC      , A.CHAIN_START_YMD      , A.CHAIN_END_YMD      , A.CS_SETUP_KEY      , A.CS_SETUP_CNT      , A.DEPT_CD      , A.CTRT_CD      , A.ELVIS_OFFICE_CD      , A.MATN_AMT      , A.CTRT_AMT      , A.IDC_AMT      , A.ETC_AMT      , (SELECT UFN_TABLE_ITEM('COM_CODE', A.TAX_TRANS_TYPE, 'C15') FROM CRM_CUST_MST B WHERE A.CUST_CD = B.CUST_CD) AS TAX_TRANS_TYPE   FROM CRM_CUST_MST A  WHERE 1=1 --동일조건 위쪽에도 추가해야함[CUST_CD]    AND A.CUST_CD = '@CUST_CD'[/CUST_CD][SEARCH_KEY]    [SEARCH_TYPE_CUST_CD]        AND A.CUST_CD = '@SEARCH_KEY'    [/SEARCH_TYPE_CUST_CD]    [SEARCH_TYPE_SVR_IP]       AND A.CUST_CD IN (                       SELECT X.CUST_CD                        FROM CRM_CUST_SERVER X                       WHERE X.SVR_IP = PKG_CRYPTO.ENCRYPT('@SEARCH_KEY','@CRYPTO_KEY' || 'Key!//#^')                  )    [/SEARCH_TYPE_SVR_IP]    [SEARCH_TYPE_SEARCH_KEY]       AND ( 1 != 1           OR A.CUST_CD = '@SEARCH_KEY'           OR A.CUST_NM LIKE '%@SEARCH_KEY%'           OR A.CUST_LOC_NM LIKE '%@SEARCH_KEY%'           OR A.CUSTOMS_CD = '@SEARCH_KEY'           OR A.CRN LIKE '@SEARCH_KEY%'           OR A.TEL_NO LIKE '%@SEARCH_KEY%'           OR A.CUST_CD IN (                      SELECT X.CUST_CD                        FROM CRM_CUST_PIC X                      WHERE (X.PIC_NM LIKE '@SEARCH_KEY%'                           OR X.TEL_NO LIKE '%@SEARCH_KEY%'                              )              )          )    [/SEARCH_TYPE_SEARCH_KEY]    [SEARCH_TYPE_KEY_WORD]       AND A.KEY_WORD LIKE '%@SEARCH_KEY%'    [/SEARCH_TYPE_KEY_WORD]    [SEARCH_TYPE_CUST_LOC_NM]       AND A.CUST_LOC_NM LIKE '%@SEARCH_KEY%'    [/SEARCH_TYPE_CUST_LOC_NM]    [SEARCH_TYPE_CUST_NM]       AND A.CUST_NM LIKE '%@SEARCH_KEY%'    [/SEARCH_TYPE_CUST_NM]    [SEARCH_TYPE_DOMAIN]       AND A.DOMAIN = '@SEARCH_KEY'    [/SEARCH_TYPE_DOMAIN]    [SEARCH_TYPE_TEL_NO]       AND A.TEL_NO LIKE '%@SEARCH_KEY%'    [/SEARCH_TYPE_TEL_NO]    [SEARCH_TYPE_CUSTOMS_CD]       AND A.CUSTOMS_CD = '@SEARCH_KEY'    [/SEARCH_TYPE_CUSTOMS_CD]    [SEARCH_TYPE_CRN]       AND A.CRN = '@SEARCH_KEY'    [/SEARCH_TYPE_CRN]    [SEARCH_TYPE_SETUP_URL]       AND A.SETUP_URL = '@SEARCH_KEY'    [/SEARCH_TYPE_SETUP_URL]    [SEARCH_TYPE_PIC_NM]       AND  A.CUST_CD IN (                   SELECT X.CUST_CD                      FROM CRM_CUST_PIC X                    WHERE (X.PIC_NM LIKE '@SEARCH_KEY%'                         OR X.TEL_NO LIKE '%@SEARCH_KEY%'                            )       )                  [/SEARCH_TYPE_PIC_NM][/SEARCH_KEY][SVC_CD]    AND A.CUST_CD IN (SELECT CUST_CD FROM CRM_CUST_SVC WHERE SVC_CD = '@SVC_CD')[/SVC_CD][USE_YN]    AND A.USE_YN = '@USE_YN'[/USE_YN][CUST_TYPE]    AND A.CUST_TYPE = '@CUST_TYPE'[/CUST_TYPE][INFO_Y]    AND EXISTS (SELECT '' FROM VT_CRM_CUST_MST_BK B WHERE A.CRN = B.CRN AND A.CUST_CD = B.CUST_CD AND B.INFO_YN = 'Y' AND B.CHECK_YN = 'Y')[/INFO_Y][INFO_N]    AND EXISTS (SELECT '' FROM VT_CRM_CUST_MST_BK B WHERE A.CRN = B.CRN AND A.CUST_CD = B.CUST_CD AND B.INFO_YN = 'N' AND B.CHECK_YN = 'Y')[/INFO_N][STOP_Y]    AND EXISTS (SELECT '' FROM VT_CRM_CUST_MST_BK B WHERE A.CRN = B.CRN AND A.CUST_CD = B.CUST_CD AND B.STOP_YN = 'Y' AND B.CHECK_YN = 'Y')[/STOP_Y][STOP_N]    AND EXISTS (SELECT '' FROM VT_CRM_CUST_MST_BK B WHERE A.CRN = B.CRN AND A.CUST_CD = B.CUST_CD AND B.STOP_YN = 'N' AND B.CHECK_YN = 'Y')[/STOP_N]--동일조건 위쪽에도 추가해야함 ORDER BY 2 DESC, A.CUST_LOC_NM
#CrmCustomerMgt@SearchRemark
--CrmCustomerMgt@SearchRemarkSELECT A.CUST_CD     , A.SEQ     , A.RMK     , A.RMK_YMD     , A.RMK_TYPE     , A.INS_USR     , TO_CHAR(A.INS_DATE,'YYYY-MM-DD hh24:mi:ss') AS INS_DATE     , A.UPD_USR     , TO_CHAR(A.UPD_DATE,'YYYY-MM-DD hh24:mi:ss') AS UPD_DATE  FROM CRM_CUST_RMK A WHERE A.CUST_CD = '@CUST_CD' ORDER BY A.INS_DATE DESC 
#CrmCustomerMgt@SearchRPAAuthKey
--CrmCustomerMgt@SearchRPAAuthKeySELECT A.MNGT_NO     , A.CUST_CD     , A.CRN     , A.CUSTOMS_CD     , A.FM_YMD     , A.TO_YMD     , A.AUTH_KEY     , A.USE_YN     --, (SELECT LOC_NM FROM CRM_USR_MST U WHERE U.USR_ID = A.INS_USR) AS INS_USR     , A.INS_USR     , TO_CHAR(TO_DATE(TO_CHAR(A.INS_YMD||A.INS_HM),'YYYY-MM-DD HH24:MI:SS'),'YYYY-MM-DD HH24:MI:SS') AS INS_DATE  FROM RPA_AUTH_KEY A --@DL_RPA WHERE A.CUST_CD = '@CUST_CD' ORDER BY A.MNGT_NO DESC
#CrmCustomerMgt@SearchSalesRemark
--CrmCustomerMgt@SearchSalesRemarkSELECT A.CUST_CD     , A.SEQ     , A.RMK     , A.RMK_YMD     , A.RMK_TYPE     , (SELECT Z.LOC_NM FROM CRM_USR_MST Z WHERE Z.USR_ID = A.INS_USR) AS INS_USR
     --A.INS_USR     , TO_CHAR(A.INS_DATE,'YYYY-MM-DD hh24:mi:ss') AS INS_DATE     , A.UPD_USR     , TO_CHAR(A.UPD_DATE,'YYYY-MM-DD hh24:mi:ss') AS UPD_DATE  FROM CRM_CUST_RMK_SALES A WHERE A.CUST_CD = '@CUST_CD' ORDER BY A.INS_DATE DESC
#CrmCustomerMgt@SearchServer
--CrmCustomerMgt@SearchServerSELECT PKG_CRYPTO.DECRYPT(A.DB_ID,'@CRYPTO_KEY' || 'Key!//#^') AS DB_ID     , PKG_CRYPTO.DECRYPT(A.DB_PORT,'@CRYPTO_KEY' || 'Key!//#^') AS DB_PORT     , PKG_CRYPTO.DECRYPT(A.DB_PWD,'@CRYPTO_KEY' || 'Key!//#^') AS DB_PWD      , PKG_CRYPTO.DECRYPT(A.DB_SID,'@CRYPTO_KEY' || 'Key!//#^') AS DB_SID     , PKG_CRYPTO.DECRYPT(A.DB_TNS,'@CRYPTO_KEY' || 'Key!//#^') AS DB_TNS     , PKG_CRYPTO.DECRYPT(A.SVR_IP,'@CRYPTO_KEY' || 'Key!//#^') AS SVR_IP     , PKG_CRYPTO.DECRYPT(A.TV_ID,'@CRYPTO_KEY' || 'Key!//#^') AS TV_ID     , PKG_CRYPTO.DECRYPT(A.TV_PWD,'@CRYPTO_KEY' || 'Key!//#^') AS TV_PWD      , PKG_CRYPTO.DECRYPT(A.WIN_ID,'@CRYPTO_KEY' || 'Key!//#^') AS WIN_ID     , PKG_CRYPTO.DECRYPT(A.WIN_PWD,'@CRYPTO_KEY' || 'Key!//#^') AS WIN_PWD     , A.*  FROM CRM_CUST_SERVER A WHERE A.CUST_CD = '@CUST_CD' 
#CrmCustomerMgt@SearchService
--CrmCustomerMgt@SearchServiceSELECT A.CUST_CD     , A.SEQ     , A.SVC_CD     , A.DEPT_CD     , A.SALES_NM     , A.CTRT_STRT_YMD     , A.CTRT_END_YMD     , A.TAX_AMT     , A.TAX_START_YMD     , A.RMK     , A.INS_USR     , TO_CHAR(A.INS_DATE,'YYYY-MM-DD hh24:mi:ss') AS INS_DATE     , A.UPD_USR     , TO_CHAR(A.UPD_DATE,'YYYY-MM-DD hh24:mi:ss') AS UPD_DATE  FROM CRM_CUST_SVC A       LEFT OUTER JOIN MDM_COM_CODE B               ON A.SVC_CD = B.COMN_CD              AND B.GRP_CD = 'C01' WHERE A.CUST_CD = '@CUST_CD' ORDER BY B.SORT 
#CrmCustomerMgt@UpdateBilling
--CrmCustomerMgt@UpdateBillingUPDATE CRM_CUST_FAX   SET UPD_USR  = '@USR_ID'	 , UPD_DATE = SYSDATE	     [SVC_TYPE], SVC_TYPE = '@SVC_TYPE'[/SVC_TYPE]     [VENDER_CD], VENDER_CD = '@VENDER_CD'[/VENDER_CD]     [FAX_NO], FAX_NO = '@FAX_NO'[/FAX_NO]     [START_YMD], START_YMD = '@START_YMD'[/START_YMD]     [END_YMD], END_YMD = '@END_YMD'[/END_YMD]     [RMK], RMK = '@RMK'[/RMK]     [BASE_RATE_AMT], BASE_RATE_AMT = @BASE_RATE_AMT[/BASE_RATE_AMT]     [RATE_AMT], RATE_AMT = @RATE_AMT[/RATE_AMT]     [FREE_CNT], FREE_CNT = @FREE_CNT[/FREE_CNT]WHERE CUST_CD = '@CUST_CD'  AND SEQ     = @SEQ
#CrmCustomerMgt@UpdateCustInfo
UPDATE CRM_CUST_DTL   SET ITEM_VAL='@ITEM_VAL' WHERE CUST_CD = '@CUST_CD'   AND ITEM_CD = '@ITEM_CD'    
#CrmCustomerMgt@UpdateCustPic
UPDATE CRM_CUST_PIC   SET PIC_NM='@PIC_NM'     , RANK='@RANK'      , TEL_NO='@TEL_NO'     , MOBILE='@MOBILE'     , EMAIL='@EMAIL'     , MAIN_YN='@MAIN_YN'     , RMK='@RMK'     , PIC_ID='@PIC_ID'     , PIC_PWD='@PIC_PWD'     , CONFIRM='@CONFIRM'     , DEPT_NM='@DEPT_NM' WHERE CUST_CD = '@CUST_CD'   AND SEQ = @SEQ    
#CrmCustomerMgt@UpdateEDIInfo
-- CrmCustomerMgt@UpdateEDIInfoUPDATE CRM_CUST_EDI   SET ID		= '@ID'     , PWD		= '@PWD'     , SENDER_ID = '@SENDER_ID'     , MAIL_BOX_ID = '@MAIL_BOX_ID'	 , UPD_USR	= '@USR_ID'	 , UPD_YMD	= UFN_DATE_FORMAT('DATE')	 , UPD_HM	= UFN_DATE_FORMAT('TIME') WHERE CUST_CD = '@CUST_CD'   AND EDI_SYS_CD = '@EDI_SYS_CD'
#CrmCustomerMgt@UpdateJoin
--CrmCustomerMgt@UpdateJoinUPDATE CRM_CUST_JOIN   SET UPD_USR  = '@USR_ID'	 , UPD_DATE = SYSDATE	     [SVC_TYPE], SVC_TYPE = '@SVC_TYPE'[/SVC_TYPE]     [VENDER_CD], VENDER_CD = '@VENDER_CD'[/VENDER_CD]     [SEND_YMD], SEND_YMD = '@SEND_YMD'[/SEND_YMD]     [START_YMD], START_YMD = '@START_YMD'[/START_YMD]     [END_YMD], END_YMD = '@END_YMD'[/END_YMD]     [RMK], RMK = '@RMK'[/RMK]WHERE CUST_CD = '@CUST_CD'  AND SEQ     = @SEQ
