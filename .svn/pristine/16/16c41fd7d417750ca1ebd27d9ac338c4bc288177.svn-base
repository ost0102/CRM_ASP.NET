<HeaderInfo> SELECT        ''     AS CUST_CD     , ''     AS CUST_NM	 , ''     AS TEL_NO     , ''     AS CTRT_STRT_YMD     , ''     AS SVC_CD	 , ''     AS INV_DAY	 , ''     AS RCV_DAY	 , ''     AS MATN_AMT	 , ''     AS JOIN_ROUTE	 , ''     AS JOIN_RMK	 , ''     AS USER_NM	 , ''     AS ETC_AMT_INFO     , ''     AS CTRT_RMK     , ''     AS EMAIL_Y     , ''     AS PIC_NM_Y     , ''     AS TEL_NO_Y     , ''     AS PIC_NM_A     , ''     AS EMAIL_A     , ''     AS TEL_NO_A    FROM DUAL;
