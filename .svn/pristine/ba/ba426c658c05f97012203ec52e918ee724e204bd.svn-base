#SysRuntimeInfo@Search
--SysRuntimeInfo@Search@SQLEXECUTE
#SysRuntimeInfo@SearchDomain
--SysRuntimeInfo@SearchDomainSELECT CUST_NM     , CUST_LOC_NM     , DOMAIN     , USR_ID     , USR_PWD     , DB_IP     , DB_PORT     , DB_SID     , CTRY_CD  FROM SYS_CUST_MST@ELVIS  WHERE TB_CHG_YN = 'Y'    AND USE_YN    = 'Y'  [DOMAIN]   AND DOMAIN = '@DOMAIN' [/DOMAIN] [SVC_CD]   AND SVC_CD = '@SVC_CD' [/SVC_CD] ORDER BY CUST_NM
