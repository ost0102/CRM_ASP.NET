#EvbClaimMgt@Close
--EvbClaimMgt@CloseBEGIN	INSERT INTO CRM_CUST_CLAIM (	       CLAIM_YM	     , SERVICE_CD 	     , CRM_CD	     , SUPPLY_AMT	     , VAT_AMT	     , NOTE	     , INS_USR	     , INS_YMD	     , INS_HM	     , UPD_USR	     , UPD_YMD 	     , UPD_HM      )VALUES( '@YEAR_MONTH'             , 'B'             , '@CRM_CD'             , @LAST_CLAIM_AMT             , @VAT_AMT             , '@NOTE'	         , '@USR_ID'	         , UFN_DATE_FORMAT('DATE')	         , UFN_DATE_FORMAT('TIME')	         , '@USR_ID'	         , UFN_DATE_FORMAT('DATE')	         , UFN_DATE_FORMAT('TIME')	     );	   	UPDATE ELVISBILL.EVB_CLAIM@EVB	   SET CLOSE_YN = 'Y'	     , UPD_USR = '@USR_ID'	     , UPD_YMD = ELVISBILL.UFN_DATE_FORMAT@EVB('DATE')	     , UPD_HM = ELVISBILL.UFN_DATE_FORMAT@EVB('TIME')	 WHERE USER_AUTH_ID = '@USER_AUTH_ID'
	   AND YEAR_MONTH = '@YEAR_MONTH';END;
#EvbClaimMgt@Search
--EvbClaimMgt@SearchSELECT A.YEAR_MONTH     , A.USER_AUTH_ID     , A.BUSN_NO           , A.BUSN_NM           , A.BASE_AMT     , A.BASE_TAX_CNT     , A.TAX_CNT     , A.TAX_PRICE     , A.TAX_CLAIM_AMT     , A.TAX_DISC_TRM_AMT     , A.INV_CNT     , A.INV_PRICE     , A.INV_CLAIM_AMT     , A.INV_DISC_TRM_AMT     , A.CRD_CNT     , A.CRD_PRICE     , A.CRD_CLAIM_AMT     , A.CRD_DISC_TRM_AMT     , A.TOT_CLAIM_AMT     , A.CLAIM_DISC_AMT     , A.LAST_CLAIM_AMT     , A.VAT_AMT     , A.LAST_CLAIM_AMT + A.VAT_AMT AS TOTAL_AMT      , A.NOTE     , A.CLOSE_YN     , B.CRM_CD  FROM EVB_CLAIM A INNER JOIN EVB_MEMB_SET B ON A.USER_AUTH_ID = B.USER_AUTH_ID WHERE YEAR_MONTH = '@YEAR_MONTH' ORDER BY BUSN_NM
#EvbClaimMgt@Update
--EvbClaimMgt@UpdateUPDATE EVB_CLAIM   SET CLAIM_DISC_AMT = @CLAIM_DISC_AMT     , LAST_CLAIM_AMT = @LAST_CLAIM_AMT     , VAT_AMT = @VAT_AMT 	 , UPD_USR = '@USR_ID'     , UPD_YMD = ELVISBILL.UFN_DATE_FORMAT@EVB('DATE')     , UPD_HM = ELVISBILL.UFN_DATE_FORMAT@EVB('TIME') WHERE YEAR_MONTH = '@YEAR_MONTH'   AND USER_AUTH_ID = '@USER_AUTH_ID'
