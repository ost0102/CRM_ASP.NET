#YjediMappingMgt@Delete
--YjediMappingMgt@DeleteDELETE YJEDI_MAPPING WHERE CUSTOMS_CD = '@CUSTOMS_CD'   AND SYS_ID = '@SYS_ID'   AND TABLE_NM = '@TABLE_NM'   AND FM_CD = '@FM_CD'   AND TO_GRP_CD = '@TO_GRP_CD'   AND TO_CD = '@TO_CD'
#YjediMappingMgt@Save
--YjediMappingMgt@Save MERGE INTO YJEDI_MAPPING A USING    (SELECT '@CUSTOMS_CD' AS CUSTOMS_CD          , '@SYS_ID' AS SYS_ID          , '@TABLE_NM' AS TABLE_NM          , '@FM_CD' AS FM_CD          , '@TO_GRP_CD' AS TO_GRP_CD          , '@TO_CD' AS TO_CD          , '@TO_NM' AS TO_NM          , '@RMK' AS RMK       FROM DUAL) B         ON (A.CUSTOMS_CD = B.CUSTOMS_CD AND A.SYS_ID = B.SYS_ID AND A.TABLE_NM = B.TABLE_NM AND A.FM_CD = B.FM_CD AND A.TO_GRP_CD = B.TO_GRP_CD AND A.TO_CD = B.TO_CD)       WHEN NOT MATCHED THEN     INSERT (INS_USR           , INS_YMD           , INS_HM           , UPD_USR           , UPD_YMD           , UPD_HM           , CUSTOMS_CD           , SYS_ID           , TABLE_NM           , FM_CD           , TO_GRP_CD           , TO_CD           , TO_NM           , RMK)     VALUES ('@USR_ID'           , UFN_DATE_FORMAT('DATE')           , UFN_DATE_FORMAT('TIME')           , '@USR_ID'           , UFN_DATE_FORMAT('DATE')           , UFN_dATE_FORMAT('TIEM')           , B.CUSTOMS_CD           , B.SYS_ID           , B.TABLE_NM           , B.FM_CD           , B.TO_GRP_CD           , B.TO_CD           , B.TO_NM           , B.RMK)       WHEN MATCHED THEN     UPDATE        SET  UPD_USR = '@USR_ID'           , UPD_YMD = UFN_DATE_FORMAT('DATE')           , UPD_HM = UFN_DATE_FORMAT('TIME')           , TO_NM = B.TO_NM           , RMK = B.RMK
#YjediMappingMgt@Search
--YjediMappingMgt@SearchSELECT A.CUSTOMS_CD     , A.SYS_ID     , A.TABLE_NM     , A.FM_CD     , A.TO_GRP_CD     , A.TO_CD     , A.TO_NM     , A.RMK     , UFN_TABLE_ITEM('USER_NM',A.INS_USR,'L','') AS INS_USR     , TO_CHAR(TO_DATE(A.INS_YMD,'YYYYMMDD'),'YYYY-MM-DD') || ' ' ||       TO_CHAR(TO_DATE(A.INS_HM,'hh24miss'), 'hh24:mi:ss') AS INS_YMD     , UFN_TABLE_ITEM('USER_NM',A.UPD_USR,'L','') AS UPD_USR     , TO_CHAR(TO_DATE(A.UPD_YMD,'YYYYMMDD'),'YYYY-MM-DD') || ' ' ||       TO_CHAR(TO_DATE(A.UPD_HM,'hh24miss'), 'hh24:mi:ss') AS UPD_YMD  FROM YJEDI_MAPPING A WHERE 1 = 1 [CUSTOMS_CD]   AND CUSTOMS_CD = '@CUSTOMS_CD' [/CUSTOMS_CD] [SYS_ID]   AND SYS_ID = '@SYS_ID' [/SYS_ID] [TABLE_NM]   AND TABLE_NM LIKE '%@TABLE_NM%' [/TABLE_NM] [FM_CD]   AND FM_CD = '@FM_CD' [/FM_CD] ORDER    BY SYS_ID DESC, TABLE_NM, FM_CD
#YjediMappingMgt@SearchChk
--YjediMappingMgt@SearchChkSELECT A.CUSTOMS_CD AS CUSTOMS_CD  FROM YJEDI_MAPPING A WHERE 1 = 1   AND A.CUSTOMS_CD = '@CUSTOMS_CD'
