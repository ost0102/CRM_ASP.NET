#CrmAsDaily@Delete
--CrmAsDaily@Delete DELETE CRM_AS_MST WHERE MNGT_NO ='@MNGT_NO'
#CrmAsDaily@Insert
-- CrmAsDaily@Insert	INSERT INTO CRM_AS_MST (		  MNGT_NO		, UPPER_MNGT_NO		, SYS_ID		, REQ_YMD		, REQ_HM		, REQ_SVC		, REQ_SVC2		, REQ_TYPE2		, CUST_CD		, CONTENT		, PROC_TYPE		, PRAR_YMD		, CMPT_YMD		, CMPT_HM		, RMK		, DEPT_CD		, REQ_USR		, ONLINE_YN		, WORK_USR
		, PROJECT_CD
		[RANK], RANK[/RANK]		[DEV_AUTH], DEV_AUTH[/DEV_AUTH]		, INS_USR		, INS_YMD		, INS_HM		, UPD_USR		, UPD_YMD		, UPD_HM
	) VALUES (	       '@MNGT_NO'	     , '@MNGT_NO'         , '@SYS_ID'         , '@REQ_YMD'         , '@REQ_HM'         , '@REQ_SVC'         , '@REQ_SVC2'         , '@REQ_TYPE2'         , '@CUST_CD'         , '@CONTENT'         , '@PROC_TYPE'         , '@PRAR_YMD'         , '@CMPT_YMD'         , '@CMPT_HM'         , '@RMK'         , '@DEPT_CD'         , '@REQ_USR'         , 'N'         , '@WORK_USR'         , '@PROJECT_CD'
		[RANK], '@RANK'[/RANK]		[DEV_AUTH], '@DEV_AUTH'[/DEV_AUTH]         , '@USR_ID'		 , TO_CHAR(SYSDATE,'yyyyMMdd')		 , TO_CHAR(SYSDATE,'HH24MISS')		 , '@USR_ID'		 , TO_CHAR(SYSDATE,'yyyyMMdd')		 , TO_CHAR(SYSDATE,'HH24MISS')	)
#CrmAsDaily@Search
--CrmAsDaily@SearchSELECT A.MNGT_NO     , A.MNGT_NO AS DB_MNGT_NO     , A.SYS_ID	 , A.REQ_YMD	 , A.REQ_HM	 , A.REQ_YMD||A.REQ_HM AS REQ_YMDHM	 , A.REQ_SVC	 , A.REQ_SVC2	 , A.REQ_TYPE2	 , A.CUST_CD	 , A.CONTENT	 , A.PROC_TYPE	 , A.PRAR_YMD	 , A.CMPT_YMD	 , A.CMPT_HM	 , A.CMPT_YMD||A.CMPT_HM AS CMPT_YMDHM	 , A.RMK	 , A.DEPT_CD	 , A.REQ_USR	 ,(SELECT CUST_LOC_NM  FROM CRM_CUST_MST WHERE CUST_CD = A.CUST_CD) AS CUST_NM
	 , A.RANK	 , A.DEV_AUTH
	 , A.DEV_AUTH AS DB_DEV_AUTH
	 , (SELECT Z.OPT_ITEM1 FROM MDM_COM_CODE Z WHERE Z.GRP_CD = 'C11' AND Z.COMN_CD = A.DEV_AUTH AND ROWNUM = 1) AS DEV_USR_ID     , NVL( NVL( ( SELECT REPLACE( WM_CONCAT(X.DEPT_CD) ,',','; ')                     FROM CRM_AS_MST_SHARE X                    WHERE X.MNGT_NO = A.MNGT_NO                      AND EXCEPT_YN   ='N' ) ,'' )             , (SELECT X.DEPT_CD FROM CRM_AS_MST X WHERE X.ORGN_MNGT_NO = A.MNGT_NO AND ROWNUM = 1)) AS DEPT_AUTH_LIST	 , A.PROJECT_CD	 ,(SELECT CD_NM FROM MDM_COM_CODE WHERE GRP_CD = 'T16' AND COMN_CD = A.PROJECT_CD) AS PROJECT_NM     , A.INS_YMD     , A.INS_HM     , A.UPD_YMD     , A.UPD_HM     , (SELECT LOC_NM FROM CRM_USR_MST U WHERE U.USR_ID = A.INS_USR) AS INS_USR     , UFN_FORMAT(A.INS_YMD,'DATE')||' '||UFN_FORMAT(A.INS_HM,'TIME') AS INS_DATE     , (SELECT LOC_NM FROM CRM_USR_MST U WHERE U.USR_ID = A.UPD_USR) AS UPD_USR     , UFN_FORMAT(A.UPD_YMD,'DATE')||' '||UFN_FORMAT(A.UPD_HM,'TIME') AS UPD_DATE  FROM CRM_AS_MST A WHERE 1=1       [REQ_FM_YMD] AND REQ_YMD >= '@REQ_FM_YMD' [/REQ_FM_YMD]        [REQ_TO_YMD] AND REQ_YMD <= '@REQ_TO_YMD' [/REQ_TO_YMD]        [PRAR_FM_YMD] AND PRAR_YMD >= '@PRAR_FM_YMD' [/PRAR_FM_YMD]        [PRAR_TO_YMD] AND PRAR_YMD <= '@PRAR_TO_YMD' [/PRAR_TO_YMD]        [CMPT_FM_YMD] AND CMPT_YMD >= '@CMPT_FM_YMD' [/CMPT_FM_YMD]        [CMPT_TO_YMD] AND CMPT_YMD <= '@CMPT_TO_YMD' [/CMPT_TO_YMD]        [INS_USR] AND INS_USR = '@INS_USR'[/INS_USR]       [DEPT_CD] AND DEPT_CD = '@DEPT_CD' [/DEPT_CD]       [PROJECT_CD] AND PROJECT_CD = '@PROJECT_CD' [/PROJECT_CD]       [MNGT_NO] AND MNGT_NO = '@MNGT_NO' [/MNGT_NO]        
#CrmAsDaily@Update
--CrmAsDaily@UpdateBEGIN UPDATE CRM_AS_MST   SET UPD_USR   = '@USR_ID'	 , UPD_YMD   = TO_CHAR(SYSDATE,'yyyyMMdd')	 , UPD_HM    = TO_CHAR(SYSDATE,'HH24MISS')	 , SAVE_CNT  = SAVE_CNT + 1	 , SYS_ID='@SYS_ID' 	 , REQ_YMD='@REQ_YMD' 	 , REQ_HM='@REQ_HM' 	 , REQ_SVC='@REQ_SVC' 	 , REQ_SVC2='@REQ_SVC2' 	 , REQ_TYPE2='@REQ_TYPE2' 	 , CUST_CD='@CUST_CD'	 , CONTENT='@CONTENT' 	 , PROC_TYPE='@PROC_TYPE'      , PRAR_YMD='@PRAR_YMD'     , CMPT_YMD='@CMPT_YMD'      , CMPT_HM='@CMPT_HM'      , RMK='@RMK' 	 , DEPT_CD='@DEPT_CD' 	 , PROJECT_CD='@PROJECT_CD' 
	 [RANK], RANK='@RANK'[/RANK]	 [DEV_AUTH], DEV_AUTH='@DEV_AUTH'[/DEV_AUTH] WHERE MNGT_NO   = '@MNGT_NO';   UPDATE CRM_AS_MST     SET DEPT_CD = '@DEPT_CD'   WHERE DEPT_CD IS NULL     AND MNGT_NO = '@MNGT_NO';    END ;
