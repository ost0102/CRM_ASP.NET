#CrmAsPrint@Print
--CrmAsPrint@PrintSELECT '회의록_' || A.MNGT_NO AS 문서번호     , (SELECT LOC_NM FROM CRM_USR_MST U WHERE U.USR_ID = A.INS_USR) AS 작성자      , UFN_FORMAT(A.REQ_YMD,'DATE')||' '||UFN_FORMAT(A.REQ_HM,'TIME')||' ~ '||UFN_FORMAT(A.CMPT_YMD,'DATE')||' '||UFN_FORMAT(A.CMPT_HM,'TIME') AS 회의일시     , (CASE WHEN INSTR(A.CMPT_RMK,'#회의장소#') IS NULL THEN ''              ELSE TRIM(REPLACE(SUBSTR(A.CMPT_RMK,INSTR(A.CMPT_RMK,'#회의장소#'),INSTR(SUBSTR(A.CMPT_RMK,INSTR(A.CMPT_RMK,'#회의장소#')),CHR(13)||CHR(10))),'#회의장소#',''))         END) AS 회의장소     , (CASE WHEN INSTR(A.CMPT_RMK,'#회의제목#') IS NULL THEN SUBSTR(A.CONTENT,1,INSTR(A.CONTENT, CHR(13)||CHR(10))-1)             ELSE TRIM(REPLACE(SUBSTR(A.CMPT_RMK,INSTR(A.CMPT_RMK,'#회의제목#'),INSTR(SUBSTR(A.CMPT_RMK,INSTR(A.CMPT_RMK,'#회의제목#')),CHR(13)||CHR(10))),'#회의제목#',''))         END) AS 회의제목     , (SELECT Z.CUST_LOC_NM FROM CRM_CUST_MST Z WHERE Z.CUST_CD = A.CUST_CD AND ROWNUM = 1) AS 참석업체외부     , '양재아이티' AS 참석업체내부     , (CASE WHEN INSTR(A.CMPT_RMK,'#참석인원외부#') IS NULL THEN ''              ELSE TRIM(REPLACE(SUBSTR(A.CMPT_RMK,INSTR(A.CMPT_RMK,'#참석인원외부#'),INSTR(SUBSTR(A.CMPT_RMK,INSTR(A.CMPT_RMK,'#참석인원외부#')),CHR(13)||CHR(10))),'#참석인원외부#',''))         END) AS 참석인원외부     , (CASE WHEN INSTR(A.CMPT_RMK,'#참석인원내부#') IS NULL THEN ''              ELSE NVL( TRIM(REPLACE(SUBSTR(A.CMPT_RMK,INSTR(A.CMPT_RMK,'#참석인원내부#'),INSTR(SUBSTR(A.CMPT_RMK,INSTR(A.CMPT_RMK,'#참석인원내부#')),CHR(13)||CHR(10))),'#참석인원내부#','')) , TRIM(REPLACE(SUBSTR(A.CMPT_RMK,INSTR(A.CMPT_RMK,'#참석인원내부#')),'#참석인원내부#','')) )          END) AS 참석인원내부     , TO_CLOB(A.CONTENT)||CHR(13)||CHR(10)||CHR(13)||CHR(10)||TO_CLOB(A.RMK) AS 회의목적내용조치방안  FROM CRM_AS_MST A WHERE A.MNGT_NO = '@MNGT_NO' --YJITS2020012271
#CrmAsPrint@XmlSchema
<MainInfo>  SELECT       '' AS 문서번호     , '' AS 작성자      , '' AS 회의일시     , '' AS 회의장소     , '' AS 회의제목     , '' AS 참석업체외부     , '' AS 참석업체내부     , '' AS 참석인원외부     , '' AS 참석인원내부     , '' AS 회의목적내용조치방안 FROM DUAL;   <UserInfo>SELECT          ''     AS CODE       , ''     AS NAME       , ''     AS ENG_NM       , ''     AS HP_NO       , ''     AS TEL_NO       , ''     AS FAX_NO       , ''     AS EMAIL       , ''     AS OFFICE_CD       , ''     AS DEPT_CD  FROM DUAL;
