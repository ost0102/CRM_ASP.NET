#HelpBwsSqlExecuter@SearchDomain
SELECT SYS.CUST_NM     , SYS.CUST_LOC_NM     , SYS.DOMAIN     , SYS.USR_ID     , SYS.USR_PWD     , SYS.DB_IP     , SYS.DB_PORT     , SYS.DB_SID     , SYS.CTRY_CD  FROM SYS_CUST_MST@ELVIS SYS  JOIN CRM_CUST_MST CRM    ON SYS.TNS_NM = CRM.DOMAIN WHERE SYS.TB_CHG_YN = 'Y'    AND SYS.USE_YN    = 'Y'    AND CRM.CUST_CD = '@CUST_CD' ORDER BY SYS.CUST_NM
#HelpBwsSqlExecuter@SearchIsu
SELECT GIM.GINO     , NVL(GIM.RPNO,DOM.RPNO) AS RPNO     , DOM.LICENSE_NO     , GID.ISSUED_TYPE     , GID.PICK_SD AS QTY     , GID.PICK_WGT AS WGT     , GID.PICK_CBM AS CBM     , GIM.EDI_REMARK     , GIM.SEND_NO     , (CASE WHEN (GIM.RPNO IS NULL AND GIM.GINO IS NOT NULL) THEN '미승인반출'              WHEN GIM.GINO IS NULL THEN '승인'             ELSE '반출' END) AS GBUN  FROM MTDO_MST DOM      FULL OUTER  JOIN MTGI_MST GIM    ON GIM.WHCD = DOM.WHCD   AND GIM.RPNO = DOM.RPNO  FULL OUTER  JOIN MTGI_DTL GID    ON GIM.WHCD = GID.WHCD   AND GIM.DONO = GID.DONO WHERE DOM.WHCD = '@WHCD'   AND DOM.GRNO = '@GRNO'   
#HelpBwsSqlExecuter@SearchRcp
SELECT GRD.GRNO     , BLM.DELNO     , BLD.LICENSE_NO     , GRD.QTY     , GRD.WGT     , GRD.CBM     , GRM.EDI_REMARK     , GRM.SEND_NO     , (CASE WHEN GRM.GRNO IS NULL THEN '미반입' ELSE '반입' END) AS GBUN     , BLM.HBLNO     , BLM.MRN||BLM.MSN||BLM.HSN AS MRN  FROM MTBL_MST BLM  JOIN MTBL_DTL BLD    ON BLM.WHCD = BLD.WHCD   AND BLM.DELNO = BLD.DELNO  LEFT  JOIN MTGR_DTL GRD    ON BLM.WHCD = GRD.WHCD   AND BLM.DELNO = GRD.DELNO  LEFT  JOIN MTGR_MST GRM    ON GRD.WHCD = GRM.WHCD   AND GRD.GRNO = GRM.GRNO WHERE BLM.WHCD = '@WHCD'   AND (BLM.HBLNO = '@HBLNO' OR (BLM.MRN||BLM.MSN||BLM.HSN) = '@MRN')
#HelpBwsSqlExecuter@SearchSql
SELECT SQL_ID     , TITLE     , SQL_STR      , APPLY_SOURCE      , (SELECT CD_NM FROM MDM_COM_CODE WHERE GRP_CD = 'H01' AND  COMN_CD =  APPLY_SOURCE) AS APPLY_SOURCE_NM     FROM SYS_ALTER_TABLE WHERE APPLY_SOURCE = '09'   AND USE_YN = 'Y' ORDER BY SQL_ID
#HelpBwsSqlExecuter@SearchWH
SELECT CODE AS WHCD , DESCR AS WHNM  FROM CO_COMMON WHERE GRPCD = 'Warehouse'
