#CrmDMPrint@PrintXML
<DETAIL>SELECT 'N' AS CHK    ,  A.CUST_CD    ,  A.CUST_ADDR    ,  A.CUST_LOC_NM AS CUST_NM    ,  (SELECT MAX(X.PIC_NM)           FROM CRM_CUST_PIC X         WHERE X.CUST_CD = A.CUST_CD           AND X.MAIN_YN = 'Y'         ) AS PIC_NM    ,  A.TEL_NO    ,  A.CEO    ,  (SELECT MAX(X.CUST_ADDR)          FROM CRM_CUST_MST X         WHERE X.CUST_CD = 'YCS00000'         ) AS OUR_ADDR    , A.ZIP_CD    , A.CUST_LOC_NM || CHR(10) || CHR(13) || A.CUST_ADDR || CHR(10) || CHR(13) || A.ZIP_CD AS CUST_INFO    , '' AS CUST_INFO1    , '' AS CUST_INFO2  FROM CRM_CUST_MST A WHERE A.USE_YN = 'Y'    AND A.CUST_CD IS NULL 
#CrmDMPrint@Search
-- CrmDMPrint@SearchSELECT 'N' AS CHK    ,  A.CUST_CD    ,  A.CUST_ADDR    ,  A.CUST_LOC_NM AS CUST_NM    ,  (SELECT MAX(X.PIC_NM)           FROM CRM_CUST_PIC X         WHERE X.CUST_CD = A.CUST_CD           AND X.MAIN_YN = 'Y'         ) AS PIC_NM    ,  A.TEL_NO    ,  A.CEO    ,  (SELECT MAX(X.CUST_ADDR)          FROM CRM_CUST_MST X         WHERE X.CUST_CD = 'YCS00000'         ) || CHR(10) || CHR(13) ||       (SELECT MAX(X.ZIP_CD)          FROM CRM_CUST_MST X         WHERE X.CUST_CD = 'YCS00000')         AS OUR_ADDR    , A.ZIP_CD    , A.CUST_LOC_NM || CHR(10) || CHR(13) || A.CUST_ADDR || CHR(10) || CHR(13) || A.ZIP_CD AS CUST_INFO  FROM CRM_CUST_MST A WHERE A.USE_YN = 'Y'  [LOC]   AND A.CTRY_CD = 'KR' [/LOC] [FRGN]   AND A.CTRY_CD NOT IN ('KR') [/FRGN] [CTRY]   AND A.CTRY_CD = '@CTRY' [/CTRY] [CUST_CD]   AND A.CUST_CD = '@CUST_CD' [/CUST_CD] [SVC_CD]   --AND A.SVC_CD = '@SVC_CD'
   AND A.CUST_CD IN (SELECT CUST_CD FROM CRM_CUST_SVC WHERE SVC_CD = '@SVC_CD') [/SVC_CD] [SALES_NM]   AND A.SALES_NM = '@SALES_NM' [/SALES_NM]  [CUST_ORDER_A] ORDER BY A.CUST_LOC_NM [/CUST_ORDER_A] [CUST_ORDER_Z] ORDER BY A.CUST_LOC_NM DESC [/CUST_ORDER_Z] 
